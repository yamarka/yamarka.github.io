[{"id": "6794ab57-3739-436f-8fc2-ce54dce9e4b6", "titulo": "El gol inesperado", "contenido": "Juan, el portero, subi\u00f3 a rematar un tiro libre en el \u00faltimo minuto. Con sorpresa, anot\u00f3 el gol del empate, salvando al equipo de la derrota.", "categoria": "1", "imagen": "https://i.ibb.co/6HpRnQT/PARQUE-CENTRAL.jpg", "enlace_noticia": "https://yamarka.github.io/todaslasnoticias/el-gol-inesperado.html"}, {"id": "fcde4e1a-4710-406e-940b-190c9d705613", "titulo": "El regate maestro", "contenido": "Luc\u00eda deslumbr\u00f3 a todos con su regate imparable. Pas\u00f3 entre rivales como si bailara, anotando el gol que los llev\u00f3 a la victoria.", "categoria": "1", "imagen": "https://www.pasiontricolor.com.uy/pasionweb2/wp-content/uploads/2021/03/PARQUE-CENTRAL.jpg", "enlace_noticia": "https://yamarka.github.io/todaslasnoticias/el-regate-maestro-fcde4e1a-4710-406e-940b-190c9d705613.html"}, {"id": "20f4949f-66d1-44eb-9b5f-1c870fbc69ba", "titulo": "El penalti decisivo", "contenido": "En la final, Mart\u00edn enfrent\u00f3 el penalti definitivo. Con nervios de acero, dispar\u00f3 al \u00e1ngulo y llev\u00f3 al equipo a la gloria.", "categoria": "1", "imagen": "https://www.pasiontricolor.com.uy/pasionweb2/wp-content/uploads/2021/03/PARQUE-CENTRAL.jpg", "enlace_noticia": "https://yamarka.github.io/todaslasnoticias/el-penalti-decisivo-20f4949f-66d1-44eb-9b5f-1c870fbc69ba.html"}, {"id": "8182c40e-1632-4804-bfe0-1ac55b91a2cb", "titulo": "El bal\u00f3n perdido", "contenido": "Sof\u00eda encontr\u00f3 un bal\u00f3n abandonado en el parque. Con sus amigos, jugaron un emocionante partido hasta la puesta de sol.", "categoria": "1", "imagen": "https://www.pasiontricolor.com.uy/pasionweb2/wp-content/uploads/2021/03/PARQUE-CENTRAL.jpg", "enlace_noticia": "https://yamarka.github.io/todaslasnoticias/el-bal\u00f3n-perdido-8182c40e-1632-4804-bfe0-1ac55b91a2cb.html"}, {"id": "81b3d08c-593c-4a98-abda-c45c19e6d063", "titulo": "El entrenamiento secreto", "contenido": "Carlos practic\u00f3 tiros libres en secreto. En el partido clave, su gol de tiro libre fue el golpe de suerte que necesitaban.", "categoria": "1", "imagen": "https://www.pasiontricolor.com.uy/pasionweb2/wp-content/uploads/2021/03/PARQUE-CENTRAL.jpg", "enlace_noticia": "https://yamarka.github.io/todaslasnoticias/el-entrenamiento-secreto-81b3d08c-593c-4a98-abda-c45c19e6d063.html"}, {"id": "5b6de50d-9a2b-48c1-84fe-f1655d2f2889", "titulo": "El pase perfecto", "contenido": "Laura vio una oportunidad \u00fanica y dio el pase perfecto. El delantero conect\u00f3 con el bal\u00f3n y marc\u00f3 el gol de la victoria.", "categoria": "1", "imagen": "https://www.pasiontricolor.com.uy/pasionweb2/wp-content/uploads/2021/03/PARQUE-CENTRAL.jpg", "enlace_noticia": "https://yamarka.github.io/todaslasnoticias/el-pase-perfecto-5b6de50d-9a2b-48c1-84fe-f1655d2f2889.html"}, {"id": "9f9e8220-f365-47f8-be62-2c724c597f21", "titulo": "El saque magistral", "contenido": "En el torneo, Diego sorprendi\u00f3 a todos con un saque desde su campo. El bal\u00f3n vol\u00f3 y entr\u00f3 directo a la porter\u00eda rival.", "categoria": "1", "imagen": "https://www.pasiontricolor.com.uy/pasionweb2/wp-content/uploads/2021/03/PARQUE-CENTRAL.jpg", "enlace_noticia": "https://yamarka.github.io/todaslasnoticias/el-saque-magistral-9f9e8220-f365-47f8-be62-2c724c597f21.html"}, {"id": "c262c9ba-8a0c-4829-b2a8-7caa1b5faa21", "titulo": "El primer partido", "contenido": "Eduardo jug\u00f3 su primer partido oficial. Con emoci\u00f3n y alegr\u00eda, corri\u00f3 tras el bal\u00f3n y vivi\u00f3 su primer gol.", "categoria": "1", "imagen": "https://www.pasiontricolor.com.uy/pasionweb2/wp-content/uploads/2021/03/PARQUE-CENTRAL.jpg", "enlace_noticia": "https://yamarka.github.io/todaslasnoticias/el-primer-partido-c262c9ba-8a0c-4829-b2a8-7caa1b5faa21.html"}]